{
    "name": "webignition/html-validator-output-parser",
    "type": "library",
    "description": "Parser for W3C HTML validator command line json output ",
    "keywords": ["html", "validator", "output", "parser"],
    "homepage": "https://github.com/webignition/html-validator-output-parser",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "webignition\\HtmlValidator\\Output\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\Tests\\HtmlValidator\\Output\\": "tests/",
            "webignition\\Tests\\HtmlValidator\\Helper\\": "tests/Helper"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7",
        "webignition/internet-media-type":">=0.4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "squizlabs/php_codesniffer": "3.*"
    }
}
